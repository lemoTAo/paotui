/* 水平间距 */
.flex_c {
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}
.flex_c_ce {
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.flex_c_s {
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
}
.flex_st {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.flex_r_no {
  display: flex;
  justify-content: center;
}
.flex_r_st {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.text {
  font-size: 26rpx;
  color: #797b7e;
}
.outer {
  padding: 40rpx 40rpx;
  box-sizing: border-box;
}
.outer .detail_box {
  padding: 50rpx 40rpx;
  height: 200rpx;
  color: #ecebfe;
  border-radius: 30rpx;
  background-color: #6154f5;
  box-sizing: border-box;
}
.outer .detail_box .detail-item {
  height: 100%;
  margin-right: 40rpx;
  font-size: 26rpx;
}
.outer .detail_box .detail-item text:last-child {
  font-size: 34rpx;
}
.outer .process-box {
  margin: auto 0;
}
.outer .process-box .proces-text {
  font-size: 44rpx;
}
.outer .process-box .process-contanter {
  position: relative;
  margin-top: 40rpx;
}
.outer .process-box .process-contanter .Dotted-line {
  position: absolute;
  width: 1px;
  height: 600rpx;
  top: 0;
  left: 50rpx;
  border-left: 2px dashed #b7b3ed;
}
.outer .process-box .process-contanter .process-list .process-item {
  height: 65px;
  margin-bottom: 40rpx;
  box-sizing: border-box;
}
.outer .process-box .process-contanter .process-list .process-item .image-box {
  width: 60px;
  height: 60px;
  margin-right: 20px;
  border-radius: 50%;
  background-color: #6154f5;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 60% 60%;
  background-position: center;
  z-index: 1;
}
.outer .process-box .process-contanter .process-list .process-item .text-item {
  padding: 20rpx 0;
  height: 100%;
  box-sizing: border-box;
}
.outer .process-box .map-container {
  position: relative;
  width: 100%;
  height: 300rpx;
  z-index: 999;
}
.outer .process-box .map-container .map {
  position: absolute;
  top: 0;
  width: 80%;
  right: 0;
  height: 300rpx;
  background-color: #b7b3ed;
  z-index: 9999;
}

