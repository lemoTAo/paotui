@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.bkActive {
  background-image: linear-gradient(to right, #679ae7, #655cf4) !important;
}
.dianActive {
  background-color: white;
  background-clip: content-box;
}
.flex_row {
  display: flex;
  justify-content: space-between;
}
.flex_co {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.outer {
  padding: 0 40rpx;
}
.outer .text_box_ {
  margin-top: 20px;
}
.outer .pay_way_box .item {
  margin-top: 20px;
  height: 160rpx;
  padding: 30rpx 40rpx;
  background-color: #f6f5fd;
  border-radius: 40rpx;
  box-sizing: border-box;
}
.outer .pay_way_box .item .image_box {
  width: 100rpx;
  height: 100rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.outer .pay_way_box .item .image_box image {
  width: 100%;
  height: 100%;
}
.outer .pay_way_box .item .text_box text:last-child {
  font-size: 26rpx;
  color: darkgray;
}
.outer .pay_way_box .item .dian_box {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.outer .pay_way_box .item .dian_box .dian {
  height: 30rpx;
  width: 30rpx;
  padding: 3px;
  border: 1px solid #d7d3f5;
  border-radius: 100%;
}
.outer .btn {
  margin-top: 70px;
  width: 200px;
  height: 50px;
  color: white;
  background-color: #665ef5;
  border-radius: 25px;
}

